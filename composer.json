{
  "name": "wyrihaximus/tactician-command-handler-mapper",
  "description": "Command to Handler mapper for Tactician",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0 || ^5.5",
    "doctrine/annotations": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^4.8",
    "squizlabs/php_codesniffer": "^2.6",
    "phake/phake": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\Tactician\\CommandHandler\\": "src/"
    },
    "files": [
      "src/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\Tactician\\CommandHandler\\": "tests/",
      "Test\\App\\": "test-app/"
    }
  },
  "config": {
    "platform": {
      "php": 5.5
    }
  }
}
